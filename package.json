    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@dnd-kit/core": "^3.1.1",
    "@dnd-kit/modifiers": "^3.0.0",
    "@dnd-kit/sortable": "^4.0.0",
    "@dnd-kit/utilities": "2.0.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@tippyjs/react": "^4.2.5",
    "codemirror": "^5.60.0",
    "faker": "^4.1.0",
    "html-react-parser": "^0.13.0",
